<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="330.0" maxWidth="500.0" minHeight="330.0" minWidth="500.0" prefHeight="330.0" prefWidth="500.0" style="-fx-background-color: white; -fx-border-color: #333;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yondu.controller.NotifController">
   <children>
      <Label fx:id="headerLabel" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" styleClass="label-1">
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </Label>
      <Line endX="350.0" startX="-100.0" stroke="#bfbaba">
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </Line>
      <Label fx:id="messageLabel" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" styleClass="label-2" wrapText="true">
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </Label>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <ImageView fx:id="iconImageView" fitHeight="100.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="okayButton" maxHeight="40.0" maxWidth="130.0" minHeight="40.0" minWidth="130.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="130.0" styleClass="primary-button" text="OK">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
